@{
    @model IEnumerable<ArtworkCategoryList>
}

<h1>Välj en kategori av alster du vill kolla närmare på:</h1>

<ul>
    @foreach (ArtworkCategoryList artworkCategoryList in Model)
    {
        <li><a href="artwork/info/@artworkCategoryList.ArtworkCategoryListId">@artworkCategoryList.CategoryName</a></li>
    }
</ul>

